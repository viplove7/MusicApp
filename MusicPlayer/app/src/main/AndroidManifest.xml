<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.viplove.music">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".HindiActivity"
            android:label="Hindi Songs"
            android:parentActivityName=".MainActivity"
           />
        <activity
            android:name=".EnglishActivity"
            android:label="English Songs"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".Englishsong"
            android:label="English Songs"
            android:parentActivityName=".Englishsong"></activity>
    </application>

</manifest>